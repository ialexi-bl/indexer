{
  "name": "indexer",
  "main": "public/electron.js",
  "version": "1.3.0-alpha",
  "description": "Application, simplifying data input for Poznan Project collborators",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "array-move": "^2.1.0",
    "babel-jest": "^24.8.0",
    "browserify-css": "^0.15.0",
    "bufferutil": "^4.0.1",
    "canvas": "^2.6.1",
    "classnames": "^2.2.6",
    "dexie": "^2.0.4",
    "electron-is-dev": "^1.1.0",
    "electron-react-titlebar": "^0.8.1",
    "exceljs": "^1.13.0",
    "i18next": "^17.0.6",
    "i18next-electron-language-detector": "^0.0.10",
    "nedb": "^1.8.0",
    "node-sass": "^4.12.0",
    "prop-types": "latest",
    "react": "^16.8.6",
    "react-beautiful-dnd": "^11.0.4",
    "react-custom-scrollbars": "^4.2.1",
    "react-data-grid": "^6.1.0",
    "react-data-grid-addons": "^6.1.0",
    "react-dom": "^16.8.6",
    "react-excel": "^1.0.0",
    "react-i18next": "^10.11.3",
    "react-ionicons": "^3.0.1",
    "react-modal": "^3.8.2",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-select": "^3.0.4",
    "react-switch": "^5.0.0",
    "react-toastify": "^5.3.1",
    "react-web-tabs": "^1.0.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "typescript": "^3.5.3",
    "utf-8-validate": "^5.0.2"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"set BROWSER=none&npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack:win": "npm run build",
    "preelectron-pack:lin": "npm run build",
    "electron-pack:win": "build -w",
    "electron-pack:lin": "build -l"
  },
  "author": {
    "name": "Alex",
    "email": "ialexi.jhc@gmail.com"
  },
  "build": {
    "appId": "com.alex.indexer",
    "productName": "Indexer",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.11",
    "@rescripts/rescript-env": "^0.0.10",
    "concurrently": "^4.1.1",
    "electron": "^5.0.6",
    "electron-builder": "^20.44.4",
    "wait-on": "^3.2.0"
  },
  "keywords": [
    "Poznan Project",
    "document",
    "Indexer"
  ]
}
